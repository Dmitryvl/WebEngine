@model IEnumerable<WebEngine.Web.ViewModels.Category.CategoryView>

@foreach (var item in Model)
{
	<div class="panel panel-default">
		<div class="panel-heading">
			<h4 class="panel-title"><a asp-action="Index" asp-controller="Product" asp-route-category="@item.Name">@item.Name</a></h4>
		</div>
	</div>
}